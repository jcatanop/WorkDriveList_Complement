{
  "manifest_version": 2,
  "name": "WorkDriveList",
  "version": "1.0",
  "description": "Help listing documents in Zoho WorkDrive",
  "icons": {
    "48": "icons/appox-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.workdrive.zoho.com/*"], 
      "js": ["workdrivelist.js"]
    }
  ]
}

